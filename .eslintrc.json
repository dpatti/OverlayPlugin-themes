{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "parser": "@babel/eslint-parser",
  "plugins": ["@babel", "react"],
  "rules": {
    "react/prop-types": "off",
    "no-undef": "off",
    "no-unused-vars": ["off"],
    "prefer-const": ["error"]
  },
  "settings": {
    "react": { "version": "detect" }
  }
}
